
#include <iostream>
#include<stdio.h>

using namespace std;

int main()
{
    int b[3];
    b[0]=1;
    b[1]=2;
    b[2]=3;
    
    int A[5]={1,2,3,4,5};
    
    for(int i=0;i<5;i++){
        printf("%d \n",A[i]);
    }
    
    for(int i=0;i<3;i++){
        cout<<b[i]<<endl;
    }

    return 0;
}
