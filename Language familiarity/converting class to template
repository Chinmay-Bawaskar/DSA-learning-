#include<stdio.h>
#include <iostream>

using namespace std;
template<class T>
class arathamatic
{
private:
    T a;
    T b;
public:
    arathamatic(T a ,T b);
    T add();

};
template<class T>
    arathamatic<T>::arathamatic(T a ,T b){
        this->a=a;
        this->b=b;
    }
template<class T>
    T arathamatic<T>::add()
    {
        return a+b;
    }
    


int main()
{
    arathamatic<char> ar('A','B');
    cout<<"add"<<(int)ar.add()<<endl;
    return 0;
}
