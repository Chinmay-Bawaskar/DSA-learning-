#include<stdio.h>
#include <iostream>

using namespace std;
struct Rectangle 
{
};
struct Rectangle *fun()
{
}

int area(int l, int b){
    return l*b;
}
int peri(int l,int b){
    int p;
    p=2*(l+b);
    return p;
}

int main()
{
    int length=0,breadth=0;
    printf("enter length and breadth");
    cin>>length>>breadth;
    
    int a=area(length,breadth);
    int p=peri(length,breadth);
    printf("area =%d\nperi=%d",a,p);
    return 0;
}
