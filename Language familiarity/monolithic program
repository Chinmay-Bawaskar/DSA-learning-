#include<stdio.h>
#include <iostream>

using namespace std;
struct Rectangle 
{
};
struct Rectangle *fun()
{
}

int main()
{
    int length=0,breadth=0;
    printf("enter length and breadth");
    cin>>length>>breadth;
    
    int area=length*breadth;
    int peri=2*(length+breadth);
    printf("area =%d\nperi=%d",area,peri);
    return 0;
}
